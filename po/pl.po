#. Description on how to use Light Display Manager displayed on command-line
#: ../src/lightdm.c:100
#, c-format
msgid ""
"Usage:\n"
"  %s - Display Manager"
msgstr ""
"Użycie:\n"
"  %s - menedżer logowania"

#. Description on how to use Light Display Manager displayed on command-line
#: ../src/lightdm.c:106
#, c-format
msgid ""
"Help Options:\n"
"  -c, --config <file>             Use configuration file\n"
"      --pid-file <file>           File to write PID into\n"
"  -d, --debug                     Print debugging messages\n"
"      --test-mode                 Run as unprivileged user\n"
"  -v, --version                   Show release version\n"
"  -h, --help                      Show help options"
msgstr ""
"Opcje:\n"
"  -c, --config <plik>             Określa plik konfiguracji\n"
"      --pid-file <plik>           Określa plik z przechowywanym PID\n"
"  -d, --debug                     Wypisuje komunikaty diagnozowania błędów\n"
"      --test-mode                 Uruchamia jako zwykły użytkownik\n"
"  -v, --version                   Wypisuje informacje o wersji\n"
"  -h, --help                      Wypisuje komunikat pomocy"
