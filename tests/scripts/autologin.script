RUNNER DAEMON-START
XSERVER :.* START
XSERVER :.* ACCEPT-CONNECT
SESSION START USER=alice
XSERVER :.* ACCEPT-CONNECT
SESSION CONNECT-XSERVER
*STOP-DAEMON
# Don't know what order they will terminate
(SESSION TERMINATE SIGNAL=15|XSERVER :.* TERMINATE SIGNAL=15|RUNNER DAEMON-EXIT STATUS=0)
(SESSION TERMINATE SIGNAL=15|XSERVER :.* TERMINATE SIGNAL=15|RUNNER DAEMON-EXIT STATUS=0)
(SESSION TERMINATE SIGNAL=15|XSERVER :.* TERMINATE SIGNAL=15|RUNNER DAEMON-EXIT STATUS=0)
